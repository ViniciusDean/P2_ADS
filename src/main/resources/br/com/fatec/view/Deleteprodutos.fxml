<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.fatec.controller.DeleteprodutosController">
    <children>
        <TableView fx:id="table_produtos" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="360.0">
            <columns>
                <TableColumn fx:id="col_codigo" prefWidth="75.0" text="Código" />
                <TableColumn fx:id="col_nome" prefWidth="100.0" text="Nome" />
                <TableColumn fx:id="col_preco" prefWidth="75.0" text="Preço" />
                <TableColumn fx:id="col_quantidade" prefWidth="75.0" text="Quantidade" />
            </columns>
        </TableView>
        <Button fx:id="btn_fechar" layoutX="295.0" layoutY="230.0" mnemonicParsing="false" onAction="#fecharJanela" text="Fechar" />
    </children>
</AnchorPane>
